<h1>Listing contact_forms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Personal email</th>
    <th>Appointment</th>
    <th>Company</th>
    <th>Email 1</th>
    <th>Email 2</th>
    <th>Email 3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contact_forms.each do |contact_form| %>
  <tr>
    <td><%= contact_form.name %></td>
    <td><%= contact_form.personal_email %></td>
    <td><%= contact_form.appointment %></td>
    <td><%= contact_form.company %></td>
    <td><%= contact_form.email_1 %></td>
    <td><%= contact_form.email_2 %></td>
    <td><%= contact_form.email_3 %></td>
    <td><%= link_to 'Show', contact_form %></td>
    <td><%= link_to 'Edit', edit_contact_form_path(contact_form) %></td>
    <td><%= link_to 'Destroy', contact_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

