<%if @error%>
	//console.log("<%= @contact_form.errors.inspect%>")
	$('input',"form#new_contact_form").removeClass("error")
	setTimeout(function() {
		<% @contact_form.errors.each do |msg| %>
        	error_validate("<%= msg %>")
    	<% end %>	
    	wait_form(2,'send_email')
	}, 800);

<%else%>
	$('form#new_contact_form').slideUp(function(){
		
	});
<%end%>